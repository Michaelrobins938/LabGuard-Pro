# Database
DATABASE_URL="postgresql://username:password@localhost:5432/labguard_pro"

# Authentication
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secure-secret-key-here"

# Email (for production)
SMTP_HOST="smtp.gmail.com"
SMTP_PORT="587"
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-app-password"
SMTP_FROM="noreply@yourdomain.com"

# API Configuration
API_BASE_URL="http://localhost:3001/api"

# AI Services
OPENAI_API_KEY="your-openai-api-key"
NEXT_PUBLIC_BIOMNI_API_KEY="your-biomni-api-key"

# OpenRouter Integration
NEXT_PUBLIC_OPENROUTER_API_KEY="your-openrouter-api-key"
NEXT_PUBLIC_OPENROUTER_BASE_URL="https://openrouter.ai/api/v1"
NEXT_PUBLIC_OPENROUTER_MODEL="anthropic/claude-3.5-sonnet"

# Payment Processing (Stripe)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="pk_test_..."
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_..."

# Stripe Price IDs (for subscription products)
STRIPE_STARTER_PRICE_ID="price_..."
STRIPE_PROFESSIONAL_PRICE_ID="price_..."
STRIPE_ENTERPRISE_PRICE_ID="price_..."

# Stripe Yearly Price IDs (for yearly subscriptions)
STRIPE_STARTER_YEARLY_PRICE_ID="price_..."
STRIPE_PROFESSIONAL_YEARLY_PRICE_ID="price_..."
STRIPE_ENTERPRISE_YEARLY_PRICE_ID="price_..."

# Biomni AI Configuration
NEXT_PUBLIC_BIOMNI_ENVIRONMENT=laboratory-management
NEXT_PUBLIC_BIOMNI_MODEL=biomni-a1-latest

# AI Assistant Configuration
NEXT_PUBLIC_ASSISTANT_ENABLED=true
NEXT_PUBLIC_ASSISTANT_PROACTIVE_MODE=true
NEXT_PUBLIC_ASSISTANT_CONTEXT_INTERVAL=5000
NEXT_PUBLIC_ASSISTANT_MAX_SUGGESTIONS=3 