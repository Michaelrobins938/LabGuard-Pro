{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "memory": 1024
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "src/server.ts" }
  ],
  "functions": {
    "src/server.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
} 